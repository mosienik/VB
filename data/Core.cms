SUB (Wing)
	WAITMS(100)
	KEYPRESS(#F4)	
	WAITMS(200)
END_SUB

SUB (Teleport)
	WAITMS(100)
	KEYPRESS(#F5)	
	WAITMS(300)
	KEYPRESS(#ENTER)
	WAITMS(200)
END_SUB

SUB (Close_Game)
	WAIT(1)
	KEYDOWN (#ALT)
	WAITMS (100)
	KEYDOWN (#F4)
	WAITMS (100)
	KEYUP (#F4)
	KEYUP (#ALT)
	WAIT(1)	
END_SUB	

SUB (Cursor_Remove)
	WAITMS(100)
	MOVE($PosW_x,$PosW_y)
END_SUB


SUB (Search_Window)
	WAITMS(100)
	WNDGETINFO(WNDFIND("Sarah Server | Gepard Shield 2.0 (^-_-^)", 0), $PosW_x, $PosW_y, $SizeW_x, $SizeW_y)
END_SUB

